{
  "name": "automated-resume-builder",
  "version": "1.0.0",
  "description": "Automated Resume Builder Pipeline using Jenkins - Generates PDF resumes using React-PDF",
  "main": "scripts/generateResume.js",
  "scripts": {
    "validate": "node scripts/validateResume.js",
    "generate": "babel-node scripts/generateResume.js",
    "test": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "dev": "npm run validate && npm run generate"
  },
  "keywords": [
    "jenkins",
    "resume",
    "automation",
    "pdf",
    "react-pdf",
    "pipeline"
  ],
  "author": "DevOps Engineer",
  "license": "MIT",
  "dependencies": {
    "@react-pdf/renderer": "^3.1.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/node": "^7.29.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
